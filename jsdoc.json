{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["README.md", "index.js", "lib/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./doc",
    "package": "package.json",
    "readme": "README.md",
    "recurse": true
  },
  "tags": {
    "dictionaries": ["jsdoc", "closure"]
  }
}